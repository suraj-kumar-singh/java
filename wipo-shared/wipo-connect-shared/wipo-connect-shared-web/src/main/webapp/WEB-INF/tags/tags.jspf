<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>
<%@ taglib prefix="tiles" uri="http://tiles.apache.org/tags-tiles" %> 
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags" %>
<%@ taglib prefix="csf" uri="/WEB-INF/tags/customFunctions" %>

<c:set var="ctx" value='<%= request.getContextPath() %>' />
<c:set var="ctxMvc" value='<%= request.getContextPath() + "/mvc"%>' />
<c:set var="ctxRes" value='<%= request.getContextPath() + "/res"%>' />
<c:set var="appLocale" value='<%= response.getLocale() %>' />

<spring:eval var="currentMillis" expression="new java.util.Date().getTime()"></spring:eval>