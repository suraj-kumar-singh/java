<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%-5p]| %d{yyyy/MM/dd HH:mm:ss.SSS} | %25c{1} | %msg%n</pattern>
		</encoder>
	</appender>


	<logger name="org.wipo.connect" level="DEBUG" />

<!-- 	<logger name="org.wipo.connect.ruleengine" level="TRACE" /> -->
<!-- 	<logger name="org.wipo.connect.scriptengine.engine" level="TRACE" /> -->

	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>